@using IBS.Utility.Enums
@using Microsoft.AspNetCore.Identity
@inject SignInManager<IdentityUser> SignInManager
@inject UserManager<IdentityUser> UserManager

@{
    string? selectedCompany = string.Empty;
    string? selectedStation = string.Empty;

    if (SignInManager.IsSignedIn(User))
    {
        selectedCompany = User.Claims.FirstOrDefault(c => c.Type == "Company")?.Value;
        selectedStation = User.Claims.FirstOrDefault(c => c.Type == "StationCode")?.Value;
    }
    var userDepartment = ViewBag.GetUserDepartment;

    var userName = UserManager.GetUserName(this.User);
}

