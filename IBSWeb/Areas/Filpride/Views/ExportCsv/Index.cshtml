@{
    ViewData["Title"] = "Export Csv";
}

<div class="container-fluid mt-4">
    <div class="d-flex justify-content-between align-items-center bg-secondary p-3 rounded shadow">
        <h2 class="text-white m-0">Export CSV</h2>
    </div>

    <div class="card shadow border-0 mt-3 p-3">
        <div class="row d-flex justify-content-start">
            <h3><b> Select what to import: </b></h3>
        </div>
        <div class="row mt-2 d-flex justify-content-start">
            <div class="col-2 d-flex justify-content-end">
                <a class="btn btn-outline-primary text-dark" asp-area="Filpride" asp-controller="ExportCsv" asp-action="ExportFilprideCollectionToCsv">Collection Csv</a>
            </div>
        </div>
    </div>
</div>